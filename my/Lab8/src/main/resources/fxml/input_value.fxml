<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="100.0" prefWidth="200.0">

    <Label layoutX="14.0" layoutY="10.0" text="VALUE:"/>
    <TextField fx:id="valueField" layoutX="70.0" layoutY="10.0" prefHeight="26.0" prefWidth="103.0" />
    <Button fx:id="confirmButton" layoutX="30" layoutY="50.0" text="%confirm.text"/>
    <Label fx:id="resultLabel" layoutX="10.0" layoutY="80.0"
           alignment="CENTER" textAlignment="CENTER" wrapText="true"/>

</AnchorPane>